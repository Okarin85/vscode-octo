{
    "name": "octo-language",
    "displayName": "octo-language",
    "description": "",
    "version": "0.0.1",
    "publisher": "hoovercj",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Languages",
        "Other",
        "Debuggers"
    ],
    "activationEvents": [
        "onLanguage:octo"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "octo.showPreview",
            "title": "Octo: Show preview"
        }, {
            "command": "octo.showPreviewToSide",
            "title": "Octo: Show preview to the side"
        }, {
            "command": "octo.showSource",
            "title": "Octo: Show source"
        }, {
            "command": "octo.openExample",
            "title": "Octo: Open an example"
        }, {
            "command": "octo.openDocs",
            "title": "Octo: Open docs"
        }],
        "languages": [{
            "id": "octo",
            "aliases": ["Octo", "octo"],
            "extensions": [".8o"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "octo",
            "scopeName": "source.octo",
            "path": "./syntaxes/octo.tmLanguage"
        }]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}